  date = {2011},
  pages = {350--5},
  keywords = {austenitic stainless steel,corrosion resistance,Ductility,fracture toughness testing,Heat treatment,rheology,Widmanstatten structure,workability},
  file = {Full Text (HTML):C\:\\Users\\MJGroup\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\ol2uaq08.default\\zotero\\storage\\3WC26R29\\SSP.172-174.html:text/html;Full Text (HTML):C\:\\Users\\MJGroup\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\ol2uaq08.default\\zotero\\storage\\GTHWZN4S\\SSP.172-174.html:text/html}
