@inproceedings{hoffman_high_2000-1,
  location = {{Orlando, FL}},
  title = {High {{Temperature Aging Characteristics}} of {{20Cr32Ni1Nb Castings}}},
  url = {https://www.onepetro.org/conference-paper/NACE-00512},
  abstract = {A metallurgical evaluation was performed on centrifugal and static 20Cr, 32Ni, 1Nb castings after 21 months aging at 1565°F (852°C) in a hydrogen reformer. The aged microstructures contained secondary precipitates at both the periphery of the eutectic niobium carbides and at intradendritic locations. Chemical analysis indicated the precipitates were M2306 chromium carbides and intermetallic niobium-nickel-silicides. Electron probe X-ray microanalysis indicated the niobium-nickel-silicides were predominately G-phase (Ni16Nb6Si7) as well as q'-phase (Nb3Ni2Si). The niobium-nickel-silicides significantly reduced the mechanical properties of the material at room and operating temperatures. The precipitated intermetallic silicides are expected to decrease the creep resistance of the cast material. Previous analysis found the niobium-nickel-silicides also promoted liquation cracking upon welding in the aged condition. The analysis results suggest the nominal chemistry of the alloy should be modified to inhibit precipitation of the detrimental intermetatlic niobium-nickel- silicides upon aging at elevated temperatures.},
  eventtitle = {CORROSION 2000},
  timestamp = {2016-07-05T01:58:07Z},
  langid = {english},
  publisher = {{NACE International}},
  author = {Hoffman, John J.},
  date = {2000-03-26},
  note = {Paper no. 00512},
  file = {Hoffman - 2000 - High Temperature Aging Characteristics of 20Cr32Ni.pdf:C\:\\Users\\MJGroup\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\ol2uaq08.default\\zotero\\storage\\M4AI9S2R\\Hoffman - 2000 - High Temperature Aging Characteristics of 20Cr32Ni.pdf:application/pdf;Full Text PDF:C\:\\Users\\MJGroup\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\ol2uaq08.default\\zotero\\storage\\QP33H3ZE\\Hoffman - 2000 - High Temperature Aging Characteristics of 20Cr32Ni.pdf:application/pdf;Snapshot:C\:\\Users\\MJGroup\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\ol2uaq08.default\\zotero\\storage\\W2AEQ6N5\\NACE-00512.html:text/html}
