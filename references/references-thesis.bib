  author = {Shi, S and Lippold, John C. and Ramirez, J},
  date = {2010-10},
  pages = {210s--217s},
  file = {SHI et al. - 2010 - Hot Ductility Behavior and Repair Weldability of S.pdf:C\:\\Users\\MJGroup\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\ol2uaq08.default\\zotero\\storage\\KCIEKCJH\\SHI et al. - 2010 - Hot Ductility Behavior and Repair Weldability of S.pdf:application/pdf}
}

