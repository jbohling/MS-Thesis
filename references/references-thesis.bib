@article{lin_model_1992,
  title = {Model for heat-affected zone liquation cracking},
  volume = {30},
  issn = {00432288},
  abstract = {A new model for quantifying heat-affected zone (HAZ) liquation cracking has been developed using the hot-ductility, the spot- and longitudinal-Varestraint tests. The model describes a thermal crack susceptible region in the HAZ in which liquation cracking may occur. This thermal crack susceptible region is material-specific and represents a true quantification of weldability. The development of this model has provided a quantitative methodology for predicting HAZ liquation cracking susceptibility, elucidated the physical relationship between weldability test results, liquation cracking theories and material properties, and clarified the interpretation of test data from the hot-ductility, the spot-and longitudinal-Varestraint tests. This paper discusses how the model is obtained and addresses the methodologies of data interpretation of the three tests. The conventional methods of data interpretation are also critically reviewed.},
  timestamp = {2013-10-31T03:17:20Z},
  number = {9-10},
  journaltitle = {Welding in the World, Le Soudage Dans Le Monde},
  shortjournal = {Welding in the World, Le Soudage Dans Le Monde},
  author = {Lin, Wangen},
  date = {1992},
  pages = {236--242},
  keywords = {Crack propagation,Heat affected zone,Welding}
