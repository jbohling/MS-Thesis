@article{patchett_welding_1998,
  title = {Welding metallurgy of {{20Cr-32Ni-Nb}} and {{HP45}} castings},
  abstract = {Repair  welding  of aged Ni-Cr castings  used  in  high  temperature  petroleum  refining  applications  is  often  disrupted  by  liquation cracking in  the  heat  affected  zone  (HAZ).  The  only  accepted  method  for  avoiding  the  problem  is  a  high  temperature  sol uti on anneal (1190°C) before welding, which is difficult to accomplish in field repairs.  Analysis  of grain  boundary  constituents in  several  aged  casting  alloys  has  shown  that  nickel and silicon diffuse to the niobium carbide boundary and then into the carbide itself  as  aging progresses.  This leads to low melting point phases of undetermined chemistry,  which  cause  the  liquation  cracking  during  welding.  There  is  no  direct  evidence  of G  phase  formation,  but  the  diffusion-affected  carbides  have  several  possible  eutectic  relationships which form liquids below 1200°C with compositions similar to the G phase  composition. Solution annealing appears to reverse the diffusion of the segregated nickel  and silicon, leaving the niobium carbide in an unaged state with good weldability.  Co-segregation of interstitial elements such as phosphorous and  sulphur was not  observed.  An apparent  concentration of phosphorous  in  the  aged  niobium  carbide  was  traced  peak  overlap  in  to  the  similar  X-ray  spectra  generated  by  niobium  and  phosphorous in the  energy dispersive X-ray analysis (EDX).  Molybdenum and  sulphur  often appear for the same reason, even when Mo is absent and sulphur segregation is not  occurring in the alloy.},
  timestamp = {2015-12-23T22:55:20Z},
  journaltitle = {Materials for Resource Recovery and Transport},
  author = {Patchett, B. M. and Skwarok, R. W.},
  date = {1998},
  pages = {379--390},
  keywords = {20Cr-32Ni-Nb},
  file = {Patchett 1998 - Welding Metallurgy of 20Cr-32Ni-Nb and HP45 Castings OCR.pdf:C\:\\Users\\MJGroup\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\ol2uaq08.default\\zotero\\storage\\XBUQ39BP\\Patchett 1998 - Welding Metallurgy of 20Cr-32Ni-Nb and HP45 Castings OCR.pdf:application/pdf}
}

