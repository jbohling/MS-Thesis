  urldate = {2014-03-20},
  date = {2012-04-01},
  pages = {1187--1197},
  keywords = {Ceramics; Glass; Composites; Natural Methods,Materials Science; general,Metallic Materials,Physical Chemistry,Structural Materials},
  file = {Full Text PDF:C\:\\Users\\MJGroup\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\ol2uaq08.default\\zotero\\storage\\DKPB2TRF\\Laha et al. - 2012 - Copper, Boron, and Cerium Additions in Type 347 Au.pdf:application/pdf;Snapshot:C\:\\Users\\MJGroup\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\ol2uaq08.default\\zotero\\storage\\EX345KK2\\s11661-011-0953-z.html:}
}

@article{young_mechanism_2008,
  title = {The mechanism of ductility dip cracking in nickel-chromium alloys},
  volume = {87},
  issn = {0043-2296},
  abstract = {High-chromium (similar to 30 wt\%) nickel-alloy filler metals are desirable for use in nuclear power systems due to their outstanding resistance to corrosion and stress corrosion cracking. However, these alloys are susceptible to welding defects, especially to subsolidus intergranular cracking commonly known as ductility dip cracking (DDC). In order to develop a high-chromium filler metal that is resistant to as-welded defects, a series of Ni-Cr alloys between 16 wt-\% and 34 wt-\% chromium were assessed for their susceptibility to cracking. Each alloy was evaluated by fabricating a restrained, multipass, automatic gas tungsten arc, V-groove weld, and counting the number of cracks per unit area observable at 50x. The type of cracking (subsolidus DDC or solidification cracking) was further differentiated via scanning electron microscopy. The results from these welds, coupled with microstructural characterization, chemical analyses, mechanical testing, microstructural modeling, and finite element modeling indicate that DDC in Ni-Cr alloys is caused by the combination of macroscopic thermal and solidification stresses induced during welding and local grain boundary stresses generated during precipitation of partially coherent (Cr,Fe)(23)C-6 carbides. Cracking can be mitigated by alloying to minimize (Cr,Fe)(23)C-6 precipitation (e.g., by Nb and Ti additions), lessening the misfit between the matrix and these precipitates (lowering the Cr and Fe concentration), and by minimizing welding-induced stresses. This mechanism of precipitation-induced cracking (PIC) via misfit stresses is consistent with subsolidus cracking in other alloy systems including superalloys, nickel-copper alloys, titanium alloys, and ferritic steels where ductility loss corresponds to the time/temperature regime where partially coherent or fully coherent second phases form.},
  timestamp = {2014-06-06T17:06:29Z},
  langid = {english},
  number = {2},
  journaltitle = {Welding Journal},
  shortjournal = {Weld. J.},
  author = {Young, G. A. and Capobianco, T. E. and Penik, M. A. and Morris, B. W. and McGee, J. J.},
  date = {2008-02},
  pages = {31S--43S},
  note = {WOS:000252660800016},
  keywords = {alloy 690,base weld metal,chromium carbides,ductility dip cracking,Embrittlement,en52,fe,high-temperature behavior,hot   ductility,intergranular cracking,ni,nickel-based alloys,part ii,strain-age cracking,subsolidus cracking,susceptibility,ti-6al-2nb-1ta-0.8mo,Weldability},
  file = {Young et al. - 2008 - The mechanism of ductility dip cracking in nickel-.pdf:C\:\\Users\\MJGroup\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\ol2uaq08.default\\zotero\\storage\\244PQ2QS\\Young et al. - 2008 - The mechanism of ductility dip cracking in nickel-.pdf:application/pdf}
}

