  pages = {536--544},
  keywords = {Artificial neural network modelling,Continuous casting,Hardness,Hot ductility,Impact toughness,In-service welding,Steels},
  file = {ScienceDirect Full Text PDF:C\:\\Users\\MJGroup\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\ol2uaq08.default\\zotero\\storage\\6I4T96AH\\Sterjovski et al. - 2005 - Artificial neural networks for modelling the mecha.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\MJGroup\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\ol2uaq08.default\\zotero\\storage\\FKZ728IW\\S0924013605006400.html:text/html}
