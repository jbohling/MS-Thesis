@article{martin_duplex_2011,
  title = {Duplex {{Stainless Steel Microstructural Developments}} as {{Model Microstructures}} for {{Hot Ductility Investigations}}},
  volume = {172-174},
  issn = {1012-0394},
  doi = {10.4028/www.scientific.net/SSP.172-174.350},
  abstract = {Duplex stainless steels (DSS) are alloys made of ferrite and austenite, with a proportion of each phase around 50\%. Their main advantage in comparison with other austenitic and ferritic stainless steels is the attractive combination of high strength and corrosion resistance together with good formability and weldability. Unfortunately, DSS often present a poor hot workability. This phenomenon can stem from different factors associated to the balance of the phases, the nature of the interface, the distribution, size and shape of the second phase, and possibly also from difference in rheology between ferrite and austenite. In order to determine the specific influence of phase morphology on the hot-workability of DSS, two austenite morphologies (E: Equiaxed and W: Widmanstatten) with very similar phase ratio have been generated using appropriate heat treatments. It was checked that the latter treatments generate stable microstructures so that subsequent hot mechanical tests are performed on the microstructures of interest. One microstructure consists of a ferritic matrix with austenitic equiaxed islands while the other microstructure is composed of a ferritic matrix with Widmanstatten austenite. The latter morphology corresponds to the morphology observed in as-cast slabs.},
  timestamp = {2016-02-15T22:29:42Z},
  journaltitle = {Diffusion and Defect Data Part B (Solid State Phenomena)},
  shortjournal = {Diffusion and Defect Data Part B (Solid State Phenomena)},
  author = {Martin, G. and Veron, M. and Chehab, B. and Fourmentin, R. and Mithieux, J.D. and Yerra, S.K. and Delannay, L. and Pardoen, T. and Brechet, Y.},
  date = {2011},
  pages = {350--5},
  keywords = {austenitic stainless steel,corrosion resistance,Ductility,fracture toughness testing,Heat treatment,rheology,Widmanstatten structure,workability},
  file = {Full Text (HTML):C\:\\Users\\MJGroup\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\ol2uaq08.default\\zotero\\storage\\3WC26R29\\SSP.172-174.html:;Full Text (HTML):C\:\\Users\\MJGroup\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\ol2uaq08.default\\zotero\\storage\\GTHWZN4S\\SSP.172-174.html:}
