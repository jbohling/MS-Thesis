  keywords = {Characterization and Evaluation Materials,Industrial Chemistry/Chemical Engineering,Mechanics,Polymer Sciences},
  file = {Snapshot:C\:\\Users\\MJGroup\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\ol2uaq08.default\\zotero\\storage\\8JHI9DWG\\BF00745578.html:;Thorvaldsson and Dunlop - 1983 - Grain boundary Cr-depleted zones in Ti and Nb stab.pdf:C\:\\Users\\MJGroup\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\ol2uaq08.default\\zotero\\storage\\KPFQJDVN\\Thorvaldsson and Dunlop - 1983 - Grain boundary Cr-depleted zones in Ti and Nb stab.pdf:application/pdf}
