  publisher = {{Publ by SAMPE}},
  author = {Lin, Wangen and Lippold, John C. and Baeslack III, William A.},
  date = {1992},
  pages = {464--476},
  keywords = {Crack propagation,Ductility,Heat affected zone,Materials testing,Thermal effects,Welding,Wetting}
}

