@article{lippold_investigation_1983,
  title = {Investigation of heat-affected zone hot cracking in {{Alloy}} 800},
  volume = {62},
  issn = {00432296},
  abstract = {The heat-affected zone (HAZ) hot cracking susceptibility of alloy 800 was evaluated using the Spot Varestraint test. Hot cracks were localized at liquated HAZ grain boundaries adjacent to the fusion line. Microprobe analysis of liquated, crack-susceptible boundaries revealed a 50-fold increase in titanium concentration relative to the bulk composition. A Ti-rich Laves phase, which exhibits a melting point approximately 50 degree C (122 degree F) below the bulk solidus temperature, was associated with the liquated boundary region. A mechanism involving the dissolution of Ti-rich carbides and assimilation of the titanium into migrating HAZ grain boundaries has been proposed to explain the HAZ hot cracking phenomenon. Both the nature and distribution of carbides (or carbonitrides) in the base metal microstructure were found to have a profound effect on the hot cracking behavior of alloy 800.},
