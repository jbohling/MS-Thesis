  author = {Shi, S. and Lippold, J.C.},
  date = {2008-08},
  pages = {1029--1040},
  keywords = {20–32Nb alloy,Cast stainless steel,Heat-resisting stainless steel,HP–Nb modified alloy,Microstructure evolution},
  file = {ScienceDirect Full Text PDF:C\:\\Users\\MJGroup\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\ol2uaq08.default\\zotero\\storage\\ISW8Q4MN\\Shi and Lippold - 2008 - Microstructure evolution during service exposure o.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\MJGroup\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\ol2uaq08.default\\zotero\\storage\\7T6M6NM9\\S1044580307003105.html:text/html}
