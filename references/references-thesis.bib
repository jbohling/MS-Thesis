@inproceedings{jaske_mechanical_2011,
  location = {{Montreal, QC, Canada}},
  title = {Mechanical integrity of primary reformer hot outlet headers},
  volume = {52},
  rights = {Compendex},
  abstract = {This paper discusses mechanical integrity issues for primary reformer hot outlet headers. These are typically welded assemblies of wrought Alloy 800H or 800HT or cast 20Cr-32Ni-Nb alloy. The typical design and operating conditions for these headers are presented Problems and failures that have been reported at past Ammonia Plant Safety Symposiums are reviewed and summarized. With this background information, recent failures of hot outlet headers are described along with the identified causes of these failures. The most common failure mechanism is high-temperature creep but the location and causes of creep damage vary. Creep failures have occurred both at weld joints and within base metal. The sources of these problems are discussed and suggestions for mitigation are presented. Based on the findings of the failure and material properties investigations, stress analysis and material damage modeling is used to estimate remaining life.},
  timestamp = {2014-06-14T17:31:08Z},
  langid = {english},
  booktitle = {Ammonia {{Plant Safety}} and {{Related Facilities}}},
  author = {Jaske, Carl E. and Quickel, Greg T.},
  date = {2011},
  pages = {83 -- 100},
  note = {Ammonia plants;Background information;Base metals;Creep damages;Creep failure;Failure mechanism;High temperature creep;Material damages;Material property;Mechanical integrity;Operating condition;Primary reformer;Remaining life;Typical design;Weld joints;Wrought alloys;},
  keywords = {Ammonia,Chromium,Creep,Niobium alloys,Stress analysis,Technical writing,WELDING,Welds}
}

@inproceedings{jaske_creep_2010,
  location = {{Prague, Czech republic}},
  title = {Creep strength of welds in hydrogen reformer outlet manifolds},
  volume = {6},
  rights = {Compendex},
  url = {http://dx.doi.org/10.1115/PVP2009-77091},
  abstract = {This paper reviews the creep strength of welds of the type typically used in hot outlet manifolds of hydrogen reformers. These manifolds can be visualized as a high-temperature piping system that collects the reformed gas from the outlets of the catalyst tubes. The catalyst tube outlet usually is connected to the manifold by means of a flexible pigtail. The pigtail is connected to the manifold via welded fittings, and the components of the manifold assembly are connected by welded joints. These hot manifolds operate under internal pressures in the range of 2000 to 3500 kPa and at temperatures in the range of 800 to 900\&deg;C The components are typically made of wrought Alloy 800, 800H or 800HT or similar cast 20Cr-32Ni-1Nb alloys. The welds are made using filler metals or electrodes of similar compositions or high-nickel alloy compositions. Creep-rupture data that have been developed for weld metals are reviewed and compared. Problems that have been encountered with failures of welds in this application are discussed. The current recommended materials for welding these manifolds are outlined. Copyright \&copy; 2009 by ASME.},
  timestamp = {2014-06-14T17:31:08Z},
  langid = {english},
  booktitle = {American {{Society}} of {{Mechanical Engineers}}, {{Pressure Vessels}} and {{Piping Division}} ({{Publication}}) {{PVP}}},
  author = {Jaske, Carl E.},
  date = {2010},
  pages = {1385 -- 1390},
  note = {Catalyst tubes;Creep strengths;Creep-rupture data;High temperature;High-nickel alloys;Internal Pressure;Weld metal;Welded joints;Wrought alloys;},
  keywords = {Alloys,BOILERS,Caissons,Catalysts,Chromium,Creep,Filler metals,Nickel alloys,Niobium alloys,Pressure vessels,Welded steel structures,Welds}
}

@inproceedings{orbons_weld_1988,
  location = {{Minneapolis, MN, USA}},
  title = {Weld cracking in reformer outlet parts after 12 years in service},
  volume = {28},
  rights = {Compendex},
  abstract = {Inspection of the reformer outlet header showed that all welds between header and crossover sockolets contained numerous fissures. Examination of the weld metal used and a stress analysis proved that these cracks were caused by the difference in mechanical and physical properties of base and weld metal. Tests to determine the creep strength of welds in low carbon cast 20Cr 32Ni 1Nb steel made with Incoweld A and equivalent welding materials showed that at temperatures above 750\&deg;C these materials had a very low creep strength, in fact much lower than the values indicated in data sheets and literature. Moreover, the creep ductility of these materials is very low. As a result, cracks will occur upon temperature variations due to thermal stresses resulting from the difference in thermal expansion.},
  timestamp = {2014-06-14T17:31:08Z},
  langid = {english},
  booktitle = {Ammonia {{Plant Safety}} (and {{Related Facilities}})},
  author = {Orbons, H.G.},
  date = {1988},
  pages = {45 -- 52},
  note = {Creep Ductility;Low Cycle Fatigue;Reformer Outlet Header;Weld Metal;},
  keywords = {Chemical Reactions–Reforming,Nickel and Alloys–Testing,Welds–Crack Propagation}
}

@inproceedings{oza_steam_2013,
  location = {{Paris, France}},
  title = {Steam methane reformer furnace outlet manifold component repair experience},
  volume = {7},
  rights = {Compendex},
  url = {http://dx.doi.org/10.1115/PVP2013-97750},
  abstract = {The steam methane reformer of RIL SEZ refinery was commissioned in 2009. Cold header replacement was planned in 2012, due to severe hot spots occurrences. While performing field welds at bull tee to cone material transitions, cracking was experienced when welding static cast tees (20Cr-32Ni-1Nb alloy) to hot header and reducers (cone). Cracks were reported in tees's heat-affected-zones. Specific weld repair procedures were developed. This paper discusses the in-service damaging mechanisms that apply to these headers including creep, thermal fatigue, thermal shock, metal dusting, thermal aging embrittlement and reheat cracking. This paper also discusses fabrication discontinuities such as solidification cracks, reheat cracks and ductility dip cracks. Then, the metallurgical analyses completed on the header components are discussed. Finally, the repairs are described including the learning steps that led to the final successful repair. The following sections of this paper depict how the innovative engineering solution was arrived at, analyzing the risks of continued operation, inspection, monitoring, and finalizing the strategy for on-line repair to sustain operation. Copyright \&copy; 2013 by ASME and Shell Oil Company.},
  timestamp = {2014-06-14T17:31:08Z},
  langid = {english},
  booktitle = {American {{Society}} of {{Mechanical Engineers}}, {{Pressure Vessels}} and {{Piping Division}} ({{Publication}}) {{PVP}}},
  author = {Oza, Shaileshkumar H. and Penso, Jorge A.},
  date = {2013},
  pages = {Pressure Vessels and Piping Division; Nondestructive Evaluation Engineering Division --},
  note = {Component repairs;Damaging mechanism;Innovative engineering;Metallurgical analysis;Reheat cracking;Solidification cracks;Steam methane reformers;Thermal aging embrittlements;},
  keywords = {Cracks,Methane,Niobium alloys,Pressure vessels,Repair,Thermal aging,Welds}
}

