  urldate = {2014-04-19},
  date = {2013-02-01},
  pages = {627--639},
  keywords = {Characterization and Evaluation of Materials,Metallic Materials,Nanotechnology,Structural Materials,Surfaces and Interfaces; Thin Films},
  file = {Snapshot:C\:\\Users\\MJGroup\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\ol2uaq08.default\\zotero\\storage\\MTV9CTQF\\s11661-012-1457-1.html:;Full Text PDF:C\:\\Users\\MJGroup\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\ol2uaq08.default\\zotero\\storage\\TAZ62JVF\\Dewar and Gerlich - 2013 - Correlation Between Experimental and Calculated Ph.pdf:application/pdf}
