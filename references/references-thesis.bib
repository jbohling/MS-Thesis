}

@article{cihal_relation_1970,
  title = {Relation between carbide precipitation and intercrystalline corrosion of stainless steels},
  volume = {10},
  issn = {0010-938X},
  url = {http://www.sciencedirect.com/science/article/pii/S0010938X70801068},
  doi = {10.1016/S0010-938X(70)80106-8},
  abstract = {The thermal effect causing a sensibility of stainless steels to the intercrystalline corrosionis accompanied by the precipitation of mixed carbides rich in chromium on the grain boundaries. In spite of various views of the mechanism of the intercrystalline corrosion it is possible to prove by the theory of depletion the relationship between the intercrystalline corrosion and the precipitation of carbides of chromium. At low temperatures the position of the Rollanson-curve corresponds to the curve of precipitation of carbides of Me23C6.
The sensibility to the intercrystalline corrosion cannot only be associated with the two-dimensional particles, but also with the carbides developed crystallographically and their distribution. These carbides are very numerous at all the temperatures and times of precipitation corresponding to a strong intercrystalline corrosion.
The creation of continuous chromium depletion in the neighbourhood of the carbides was found to be undoubtedly the primary cause of intercrystalline attacks. It is indicated that studies of the subject relation are most conveniently made with the aid of extraction replicas or possibly thin metal foils as specimens for electron microscopy.
L'effet thermique qui est la cause de la sensibilité des aciers inoxydables à la corrosionintercristalline est accompagné de la précipitation de carbures mixtes riches en chrome sur les régions limitrophes du grain. En dépit de points de vue divers du mécanisme de la corrosion intercristalline, il est possible de prouver par la théorie d'amoindrissement la relation entre la corrosion intercristalline et la précipitation de carbures de chrome. A basses températures la position de la courbe de Rollanson correspond à la courbe de précipitation de carbures de Me23C6.
La sensibilité à la corrosion intercristalline ne peut seulement être associée aux particules bi-dimensionellesmais aussi aux carbures développés cristallographiquement et leur distribution. Ces carbures sont très nombreux à toutes les températures et temps de précipitation correspondant à une forte corrosion intercristalline.
La création d'un abaissement continuel de chrome dans le voisinage des carbures a été trouvéeêtre sans aucun doute la cause principale d'attaques intercristallines. Il est indiqué l'étude de la relation du sujet est le mieux menée à l'aide de reproductions d'extraction ou encore à l'aide de pellicules métalliques minces comme échantillons pour la microscopic par électrons.
Die Wärmewirkung, welche eine Sensibilität von rostfreiem Stahl für interkristalline Korrosion verursacht, wird von der Ausscheidung gemischter, an Chrom reichen Kohlenstoffmetalle an den Korngrenzen begleitet. Trotz der verschiedenen Ansichten über die Arbeitsweise interkristalliner Korrosion ist es möglich, durch die Theorie der Abreicherung die Beziehung zwischen der interkristallinen Korrosion und der Ausscheidung von Chromkohlenstoffmetallen zu beweisen. Bei niedrigen Temperaturen entspricht die Lage der Rollanson Kurve der Ausscheidungskurve von Kohlenstoffmetallen des Me23C6.
Die Sensibilität der interkristallinen Korrosion kann nicht nur mit den zwei-dimensionalen Partikeln verbunden werden, sondern auch mit den kristallographisch entwickelten Kohlenstoffmetallen und ihrer Verteilung. Diese Kohlenstoffmetalle sind bei allen Temperaturen und zu allen Zeiten der Ausscheidung sehr zahlreich, entsprechend einer starken, interkristallinen Korrosion.
Die Bildung ständiger Chromabreicherung nahe den Kohlenstoffmetallen fand man zweifellos als den Hauptgrund für interkristallinen Angriff. Es wird darauf hingewiesen, dass Untersuchungen der gegenständlichen Beziehung am besten mit Hilfe von Extraktionsnachbildungen oder möglicherweise dünnen Metallfolien als Probe für Elektronenmikroskopie gemacht werden.},
  timestamp = {2014-03-19T15:03:58Z},
  number = {12},
  journaltitle = {Corrosion Science},
  shortjournal = {Corrosion Science},
  author = {Ĉíhal, VladimíR and Kašová, Irena},
  date = {1970},
  pages = {875--881},
  file = {Ĉíhal and Kašová - 1970 - Relation between carbide precipitation and intercr.pdf:C\:\\Users\\MJGroup\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\ol2uaq08.default\\zotero\\storage\\8A2MPDNA\\Ĉíhal and Kašová - 1970 - Relation between carbide precipitation and intercr.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\MJGroup\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\ol2uaq08.default\\zotero\\storage\\2PPJPZI5\\S0010938X70801068.html:text/html}
}

@article{thorvaldsson_grain_1983,
  title = {Grain boundary {{Cr}}-depleted zones in {{Ti}} and {{Nb}} stabilized austenitic stainless steels},
  volume = {18},
