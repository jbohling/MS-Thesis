  file = {Full Text PDF:C\:\\Users\\MJGroup\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\ol2uaq08.default\\zotero\\storage\\D4EKTIPK\\DuPont - 2011 - Application of Solidification Models for Controlli.pdf:application/pdf;Snapshot:C\:\\Users\\MJGroup\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\ol2uaq08.default\\zotero\\storage\\HQP6XKQ5\\978-3-642-16864-2_14.html:text/html},
